{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "index.html",
    "assetGroups": [
      {
        "name": "app",
        "installMode": "prefetch",
        "resources": { "files": ["/favicon.ico", "/*.css", "/*.js", "/*.webmanifest"] }
      },
      {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": { "files": ["/assets/**", "/*.(png|jpg|jpeg|svg|webp)"] }
      }
    ]
  }
  